{
  "extends": "standard",
  "plugins": ["html"],
  "parser": "babel-eslint",
  "rules": {
    "no-new": "off",
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "eol-last": 0,
    "space-before-function-paren": [
      "error",
      { "anonymous": "always", "named": "never", "asyncArrow": "always" }
    ],
    "no-trailing-spaces": ["warn", { skipBlankLines: true }],
    "semi": ["error", "always"]
  }
}
